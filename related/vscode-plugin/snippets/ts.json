{
  "TestSuite": {
    "prefix": "suite",
    "body": [
      "import assert from 'node:assert';",
      "",
      "import {Suite, Test} from '@travetto/test';",
      "",
      "@Suite()",
      "class ${1:name}Suite {",
      "  @Test()",
      "  async simpleTest() {",
      "    assert(true);",
      "  }",
      "}"
    ]
  }
}
