{
  "name": "@travetto/mono-repo",
  "type": "module",
  "author": {
    "email": "travetto.framework@gmail.com",
    "name": "Travetto Framework"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "private": true,
  "license": "MIT",
  "scripts": {
    "prestart": "npm ci",
    "pretest": "trv service start",
    "start": "trvc clean; trvc start",
    "test": "trv test -f tap-summary",
    "lint": "trv eslint",
    "reinit-deps": "find . -name 'node_modules' -type d | grep -v -E '(github[.]io|vscode-plugin|[.]trv)' | xargs rm -rf; rm package-lock.json; npm i",
    "vscode:package": "cd related/vscode-plugin; npm run package",
    "version": "trv repo:version",
    "preversion": "trv doc:angular"
  },
  "travetto": {
    "build": {
      "compilerUrl": "http://127.0.0.1:26803"
    },
    "doc": {
      "baseUrl": "https://github.com/travetto/travetto/tree/main",
      "outputs": [
        "README.md",
        "DOC.html"
      ]
    }
  },
  "workspaces": [
    "module/*",
    "global-test/*",
    "related/todo-app"
  ],
  "overrides": {
    "whatwg-url": "$whatwg-url",
    "debug": "$debug"
  },
  "devDependencies": {
    "@travetto/repo": "^7.0.5",
    "@types/node-forge": "^1.3.14",
    "chalk": "^5.6.2",
    "debug": "^4.4.3",
    "node-forge": "^1.3.3",
    "rollup-plugin-visualizer": "^6.0.5",
    "whatwg-url": "15.1.0"
  },
  "version": "0.0.0"
}
