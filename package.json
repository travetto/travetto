{
  "author": {
    "email": "travetto.framework@gmail.com",
    "name": "Travetto Framework"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "private": true,
  "name": "@travetto/mono-repo",
  "license": "MIT",
  "scripts": {
    "prestart": "npm ci --legacy-peer-deps",
    "start": "./bin/setup.sh",
    "test": "trv repo:test",
    "lint": "npx eslint --ext .js --ext .ts .",
    "upgrade-deps": "npm update -S"
  },
  "travetto": {
    "docBaseUrl": "https://github.com/travetto/travetto/tree/main"
  },
  "travettoRepo": {
    "globalModules": [
      "module/test",
      "module/cli",
      "module/compiler",
      "module/command",
      "module/repo"
    ],
    "globalTests": [
      "global-test/asset-rest",
      "global-test/auth-rest",
      "global-test/model_asset",
      "global-test/model_auth-model",
      "global-test/model_cache",
      "global-test/model_rest-session",
      "global-test/openapi",
      "global-test/rest-session",
      "global-test/transformer"
    ],
    "docRelated": [
      "related/todo-app",
      "related/overview",
      "related/vscode-plugin"
    ]
  },
  "workspaces": [
    "module/*"
  ],
  "dependencies": {
    "@types/eslint": "^8.4.10",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-plugin-unused-imports": "^2.0.0"
  },
  "overrides": {
    "compression": {
      "debug": "$debug"
    },
    "express": {
      "debug": "$debug",
      "body-parser": {
        "debug": "$debug"
      },
      "final-handler": {
        "debug": "$debug"
      },
      "send": {
        "debug": "$debug"
      }
    }
  },
  "devDependencies": {
    "node-forge": "^1.3.1",
    "@types/node-forge": "^1.3.0",
    "@parcel/watcher": "^2.0.7",
    "@aws-sdk/client-ses": "^3.215.0",
    "@types/passport-facebook": "^2.1.11",
    "passport-facebook": "^3.0.0",
    "debug": "^4.3.4",
    "@types/debug": "^4.1.7"
  }
}
