	"scripts": {
		"lint": "npx eslint --ignore-pattern '**/*.spec.ts' --ignore-pattern '**/out/**' --ignore-pattern '**/api-client/**' --ignore-pattern '**/build/**' --ignore-pattern '**/dist/**' --ignore-pattern '**/node_modules/**' --ext .js --ext .ts .",
		"init": "./bin/run.sh dev-init",
		"docs": "./bin/docs/gen.sh",
		"reinit": "rm -rf node_modules/; npm i; npm run init",
		"service": "./bin/test/resources.sh",
		"test": "cd module/base; npx trv test:lerna",
		"prepare": "cd module/boot; npm run prepare",
		"boot": "cd module/boot; npm run watch"
	},
	"devDependencies": {
		"@types/faker": "^4.1.12",
		"@types/mysql": "^2.15.15",
		"@types/node-forge": "^0.9.4",
