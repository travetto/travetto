{
  "author": {
    "email": "travetto.framework@gmail.com",
    "name": "Travetto Framework"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "type": "commonjs",
  "private": true,
  "name": "@travetto/mono-repo",
  "license": "MIT",
  "scripts": {
    "prestart": "npm ci",
    "pretest": "npx trv service start",
    "start": "trvc clean; trvc start ",
    "test": "trv test -f tap-streamed",
    "lint": "trv lint",
    "upgrade-deps": "npm update -S",
    "reinit-deps": "find . -name 'node_modules' -type d | grep -v -E '(github[.]io|vscode-plugin|[.]trv)' | xargs rm -rf; rm package-lock.json; npm i",
    "vscode:package": "cd related/vscode-plugin; npm run package"
  },
  "travetto": {
    "build": {
      "compilerUrl": "http://127.0.0.1:26803"
    },
    "doc": {
      "baseUrl": "https://github.com/travetto/travetto/tree/main",
      "outputs": [
        "README.md",
        "DOC.html"
      ]
    }
  },
  "workspaces": [
    "module/*",
    "global-test/*",
    "related/todo-app"
  ],
  "overrides": {
    "node-fetch": {
      "whatwg-url": "14.0.0"
    },
    "mongodb": {
      "whatwg-url": "14.0.0"
    },
    "compression": {
      "debug": "^4.3.4"
    },
    "express": {
      "debug": "^4.3.4",
      "body-parser": {
        "debug": "^4.3.4"
      },
      "final-handler": {
        "debug": "^4.3.4"
      },
      "send": {
        "debug": "^4.3.4"
      }
    }
  },
  "devDependencies": {
    "@travetto/repo": "^5.0.0-rc.10",
    "@types/node-forge": "^1.3.11",
    "@types/vscode": "^1.92.0",
    "node-forge": "^1.3.1",
    "rollup-plugin-visualizer": "^5.12.0"
  }
}
