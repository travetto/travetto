{
  "author": {
    "email": "travetto.framework@gmail.com",
    "name": "Travetto Framework"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "type": "commonjs",
  "private": true,
  "name": "@travetto/mono-repo",
  "license": "MIT",
  "scripts": {
    "prestart": "npm ci --legacy-peer-deps",
    "start": "npx tsc -p eslint/tsconfig.json",
    "test": "trv repo:test",
    "lint": "npx eslint --ext .js --ext .ts .",
    "upgrade-deps": "npm update -S"
  },
  "travetto": {
    "globalModules": [
      "module/test",
      "module/cli",
      "module/compiler",
      "module/doc"
    ],
    "docBaseUrl": "https://github.com/travetto/travetto/tree/main"
  },
  "workspaces": [
    "module/*",
    "module/*/doc-exec",
    "global-test/*",
    "related/overview",
    "related/todo-app",
    "related/vscode-plugin"
  ],
  "overrides": {
    "compression": {
      "debug": "$debug"
    },
    "express": {
      "debug": "$debug",
      "body-parser": {
        "debug": "$debug"
      },
      "final-handler": {
        "debug": "$debug"
      },
      "send": {
        "debug": "$debug"
      }
    }
  },
  "devDependencies": {
    "@aws-sdk/client-ses": "^3.215.0",
    "@parcel/watcher": "^2.0.7",
    "@travetto/repo": "*",
    "@types/debug": "^4.1.7",
    "@types/eslint": "^8.4.10",
    "@types/node-forge": "^1.3.0",
    "@types/passport-facebook": "^2.1.11",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "debug": "^4.3.4",
    "eslint": "^8.28.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "node-forge": "^1.3.1",
    "passport-facebook": "^3.0.0"
  }
}
