{
  "name": "@travetto/boot",
  "displayName": "Boot",
  "version": "3.0.0-rc.0",
  "description": "Bootstrapping and common utilities for travetto applications.",
  "keywords": [
    "bootstrapping",
    "caching",
    "travetto",
    "typescript"
  ],
  "homepage": "https://travetto.io",
  "license": "MIT",
  "author": {
    "email": "travetto.framework@gmail.com",
    "name": "Travetto Framework"
  },
  "files": [
    "src",
    "support",
    "tsconfig.trv.json"
  ],
  "main": "index.ts",
  "repository": {
    "url": "https://github.com/travetto/travetto.git",
    "directory": "module/boot"
  },
  "scripts": {
    "build": "tsc -p src-ts",
    "build:watch": "tsc -w -p src-ts",
    "compress": "trv-compress-js '{module/boot/src/*,module/boot/src/internal/*}'",
    "prepack": "npm run build && npm run compress"
  },
  "dependencies": {
    "@types/node": "^18.8.2",
    "@types/source-map-support": "^0.5.6",
    "source-map-support": "^0.5.21",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "trvDependencies": {
    "@travetto/test": [
      "test"
    ]
  },
  "engines": {
    "node": ">=12.17.0"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  }
}
